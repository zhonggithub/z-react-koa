{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-param-reassign": 0,
    "max-len": [1, 400],
    "no-await-in-loop": 0,
    "global-require": 0,
    "no-restricted-syntax": ["error", "FunctionExpression", "WithStatement"],
    "import/no-dynamic-require": 0
  }
}